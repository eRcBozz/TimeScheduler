<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Light.Distant?>
<?import javafx.scene.effect.Lighting?>
<?import javafx.scene.effect.Shadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.timescheduler.UserLoginController">
   <children>
      <AnchorPane prefHeight="56.0" prefWidth="600.0" style="-fx-background-color: #ff4040;">
         <children>
            <Text layoutX="274.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
      <Button fx:id="logoutButton" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#logoutButtonOnAction" text="LOGOUT">
         <effect>
            <Lighting diffuseConstant="1.7">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant>
                     <color>
                        <Color red="1.0" green="0.250980406999588" blue="0.250980406999588" />
                     </color>
                  </Light.Distant>
               </light>
            </Lighting>
         </effect></Button>
      <Button fx:id="exitButton" layoutX="523.0" layoutY="361.0" mnemonicParsing="false" onAction="#exitButtonOnAction" prefHeight="25.0" prefWidth="63.0" text="EXIT">
         <effect>
            <Lighting diffuseConstant="1.7">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant>
                     <color>
                        <Color red="1.0" green="0.250980406999588" blue="0.250980406999588" />
                     </color>
                  </Light.Distant>
               </light>
            </Lighting>
         </effect></Button>
   </children>
</AnchorPane>
